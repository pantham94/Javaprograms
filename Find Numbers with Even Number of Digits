    public int findNumbers(int[] nums) {
        int count=0;
        for(int n:nums){
            if(getDigits(n)%2==0){
                count++;
            }
        }
        return count;
    }
    int getDigits(int n){
        int digits = 0;
        while(n!=0){
            n=n/10;
            digits++;
        }
        return digits;
    }
